{
    "git.autofetch": true,
    "git.confirmSync": false,
    "editor.quickSuggestions": false,
    "editor.parameterHints.enabled": false,

    "files.exclude": {
        "**/__pycache__": true
    },
    "editor.suggestOnTriggerCharacters": false,
    "python.formatting.autopep8Args": [
        "--max-line-length 100"
    ],
}
